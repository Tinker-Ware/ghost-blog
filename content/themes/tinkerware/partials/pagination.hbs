{{! This is the pagination - it will be output using this markup }}

{{! PAGINATION START }}
<ul class="pagination" role="navigation" aria-label="Pagination">
    {{#if prev}}
        <li><a href="{{page_url next}}" aria-label="Next page">&laquo; Newer <span class="show-for-sr">page</span></a></li>
    {{/if}}
    {{#if next}}
        <li><a href="{{page_url next}}" aria-label="Next page">Older <span class="show-for-sr">page</span>&raquo;</a></li>
    {{/if}}
</ul>
{{! PAGINATION END }}